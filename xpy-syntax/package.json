{
  "name": "xpy",
  "displayName": "xpy_syntax",
  "description": "Python HTML syntax extensions",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "xpy",
      "aliases": ["XPy", "xpy"],
      "extensions": [".xpy"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "xpy",
      "scopeName": "source.xpy",
      "path": "./syntaxes/xpy.tmLanguage.json"
    }]
  }
}
